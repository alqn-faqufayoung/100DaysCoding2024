package coding100days;

public class day12 {
    //data integer

    public static void main(String[] args) {
        //range yg bisa di jangkau oleh tipe data int:
        int minimal = -2147483648;
        int maximal = 2147483647;
        /* Tipe data int adalah tipe data primitif di java yg digunakan
       untuk menyimpan bilangan bulat, tipe data ini lebih umum digunakan
       untuk angka bulat,ukuran lebih besar dan rentang yg lebih besar
       dibandingkan dengan tipe data short/byte.
         */
        System.out.println("range nilai/angka tipe data int : ");
        System.out.println("nilai/angka paling kecil : " + minimal);
        System.out.println("nilai/angka paling besar : " + maximal);
    }

}
